Board = import "board.ptls"
External = import "external.ptls"

board = Board.new()
ext1 = External.new("p1")
ext2 = External.new("p2")

fn show(board, ext1, ext2)
  ext1 |= External.show
  ext2 |= External.show

  "

           1 2 3
       f $ext2

  $board

       e $ext1
           1 2 3

  "
end

Console.debug(board)
