coins = Table.of({
  name: ["Quarter", "Dime", "Nickle", "Penny"],
  value: [25, 10, 5, 1],
})

cents = Str.toNum(prompt("Enter cents: "))

print("-" * 10)

for coin in coins do
  count = Math.floor(cents / coin.value)
  cents -= count * coin.value
  plural = if count == 1 then "" else "s" end
  print("$count $coin.name$plural")
end
