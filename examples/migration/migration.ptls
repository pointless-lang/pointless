cities = import "csv:cities.csv"

cities
  | select("state")
  | table.counts
  | take(10)
  | print

cities.change = cities $ roundTo(arg.pop2024 / arg.pop2020 * 100 - 100, 2)

cities.density = cities $ roundTo(arg.pop2024 / arg.area, 2)

cities
  | table.top("change", 10)
  | print

cities
  | table.top("density", 10)
  | print
